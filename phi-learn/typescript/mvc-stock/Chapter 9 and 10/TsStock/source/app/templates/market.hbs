<div class="panel panel-default fadeInUp animated">
  <div class="panel-body">
    <h2>{{market}}</h2>
    <table class="table table-responsibe table-condensed">
      <thead>
        <tr>
          <th>Symbol</th>
          <th>Name</th>
          <th>Last Sale</th>
          <th>Market Capital</th>
          <th>IPO year</th>
          <th>Sector</th>
          <th>industry</th>
          <th>Quote</th>
        </tr>
      </thead>
      <tbody>
        {{#each items}}
          <tr>
            <td><span class="label label-default">{{Symbol}}</span></td>
            <td>{{{Name}}}</td>
            <td>{{LastSale}}</td>
            <td>{{MarketCap}}</td>
            <td>{{IPOyear}}</td>
            <td>{{Sector}}</td>
            <td>{{industry}}</td>
            <td>
              <button class="btn btn-primary btn-sm getQuote" data-symbol="{{Symbol}}">
                <span class="glyphicon glyphicon-stats" aria-hidden="true"></span>
                Quote
              </button>
            </td>
          </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</div>
