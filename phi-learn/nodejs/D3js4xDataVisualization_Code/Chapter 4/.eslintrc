{
  extends: "airbnb",
  parser: "babel-eslint",
  "plugins": [
    "babel"
  ],
  rules: {
    "newline-per-chained-call": ['error', {"ignoreChainWithDepth": 1}],
    "babel/generator-star-spacing": 1,
    "generator-star-spacing": 0,
    "no-console": 0,
    "no-unused-vars": 1,
  },
  "root": true,
  "settings": {},
  env: {
    browser: true
  }
}
